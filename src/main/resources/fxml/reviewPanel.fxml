<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<VBox xmlns:fx="http://javafx.com/fxml" xmlns="http://javafx.com/javafx"
      fx:controller="ui.controller.sidebar.ReviewPaneController">
    <Label fx:id="moveNumber"/>
    <HBox AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0">
        <AnchorPane>
            <Button fx:id="backButton" mnemonicParsing="false" text="Back" AnchorPane.leftAnchor="0"
                    AnchorPane.rightAnchor="0"/>
        </AnchorPane>
        <AnchorPane>
            <Button fx:id="forwardButton" mnemonicParsing="false" text="Forward" AnchorPane.leftAnchor="0"
                    AnchorPane.rightAnchor="0"/>
        </AnchorPane>
    </HBox>
</VBox>