<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<HBox xmlns:fx="http://javafx.com/fxml" xmlns="http://javafx.com/javafx" fx:id="scorePane" prefHeight="46.0"
      prefWidth="175.0" fx:controller="ui.controller.sidebar.ScorePaneController">
    <VBox id="blackScoreBox" prefHeight="46.0" prefWidth="100.0">
        <Label text="Black Score"/>
        <Label fx:id="blackScore" text="0"/>
        <Button fx:id="blackDone" text="Done"/>
    </VBox>
    <Separator id="scoreSeparator" orientation="VERTICAL" prefHeight="46.0"/>
    <VBox prefHeight="46.0" prefWidth="100.0">
        <Label text="White Score"/>
        <Label fx:id="whiteScore" text="0"/>
        <Button fx:id="whiteDone" text="Done"/>
    </VBox>
</HBox>
