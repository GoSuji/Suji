<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.text.Font?>
<FlowPane xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.controller.OGSPlayer"
          xmlns="http://javafx.com/javafx/8.0.111">
    <ImageView fx:id="avatar" fitHeight="128.0" fitWidth="128.0" pickOnBounds="true" preserveRatio="true"/>
    <Label fx:id="username" text="Username">
        <font>
            <Font size="24.0"/>
        </font>
    </Label>
    <Label fx:id="rank" text="Rank">
        <font>
            <Font size="18.0"/>
        </font>
        <padding>
            <Insets left="5.0"/>
        </padding>
    </Label>
</FlowPane>
