<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.*?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.121"
            fx:controller="ui.BoardController">
    <center>
        <SplitPane dividerPositions="0.7" prefHeight="400" prefWidth="600.0">
            <items>
                <Pane fx:id="pane" minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0"/>
                <VBox prefHeight="200.0" prefWidth="100.0">
                    <children>
                        <AnchorPane>
                            <children>
                                <Button mnemonicParsing="false" onAction="#pass" text="Pass" AnchorPane.leftAnchor="0"
                                        AnchorPane.rightAnchor="0"/>
                            </children>
                        </AnchorPane>
                    </children>
                </VBox>
            </items>
        </SplitPane>
    </center>
</BorderPane>
